{
  "compileOnSave": false,
  "compilerOptions": {
    "rootDir": ".",
    "sourceMap": true,
    "declaration": true,
    "outDir": "dist",
    "moduleResolution": "node",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "importHelpers": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "target": "es2015",
    "module": "commonjs",
    "lib": [
      "es2017",
      "dom"
    ],
    "skipLibCheck": true,
    "declarationMap": true,
    "noImplicitAny": false,
    "allowSyntheticDefaultImports": true,
    "paths": {
      "@ssegning/multer-transform": [
        "multer-transform/src/index.ts"
      ],
      "@ssegning/multer-transform-sharp": [
        "multer-transform-sharp/src/index.ts"
      ]
    },
    "skipDefaultLibCheck": true,
    "baseUrl": "."
  },
  "exclude": [
    "node_modules"
  ]
}
